/* pages/register-detail/register-detail.wxss */

.p-register-detail {
  font-size: 28rpx;
  color: #333;
}

.card-header {
  padding-bottom: 10rpx;
  text-align: center;
}

.qrcode {
  width: 280rpx;
  height: 280rpx;
  margin: 0 auto 30rpx;
}

.qrcode image {
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
}

.item {
  display: flex;
}

.key{
  flex: 0 0 150rpx;
  padding: 0 20rpx;
  text-align: right;
  color: #999;
}

.val{
  flex: 1 1 auto;
}
